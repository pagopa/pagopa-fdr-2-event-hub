{
  "version": "2.0",
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[4.0.0, 5.0.0)"
  },
  "extensions": {
    "blobs": {
      "maxDegreeOfParallelism": 1,
      "poisonBlobThreshold": 2
    },
    "queues": {
      "maxPollingInterval": "00:00:45",
      "visibilityTimeout": "00:10:00",
      "batchSize": 1,
      "maxDequeueCount": 3,
      "newBatchThreshold": 1
    },
    "http": {
      "routePrefix": ""
    }
  },
  "logging": {
    "fileLoggingMode": "debugOnly",
    "logLevel": {
      "default": "None",
      "Host.Results": "Error",
      "Function.ProcessFDR1BlobFiles": "Information",
      "Function.ProcessFDR3BlobFiles": "Information",
      "Host.Aggregator": "Error"
    },
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "maxTelemetryItemsPerSecond": 5,
        "includedTypes": "PageView;Trace;Dependency;Request",
        "excludedTypes": "Exception;Event;CustomEvent"
      }
    }
  }
}
